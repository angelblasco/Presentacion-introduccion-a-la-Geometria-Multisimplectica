@misc{djounvouna_observables_2025,
	title = {Observables of Relative Structures and Lie 2-algebras associated with Quasi-Hamiltonian \$G\$-spaces},
	url = {http://arxiv.org/abs/2509.08153},
	doi = {10.48550/arXiv.2509.08153},
	abstract = {A manifold is said to be \$n\$-plectic if it is equipped with a closed, nondegenerate \$(n+1)\$-form. This thesis develops the theory of {\textbackslash}emph\{relative \$n\$-plectic structures\}, where the classical condition is replaced by a closed, nondegenerate {\textbackslash}emph\{relative\} \$(n+1)\$-form defined with respect to a smooth map. Analogous to how \$n\$-plectic manifolds give rise to \$L\_{\textbackslash}infty\$-algebras of observables, we show that relative \$n\$-plectic structures naturally induce corresponding \$L\_{\textbackslash}infty\$-algebras. These structures provide a conceptual bridge between the frameworks of quasi-Hamiltonian \$G\$-spaces and \$2\$-plectic geometry. As an application, we examine the relative \$2\$-plectic structure canonically associated to quasi-Hamiltonian \$G\$-spaces. We show that every quasi-Hamiltonian \$G\$-space defines a closed, nondegenerate relative \$3\$-form, and that the group action induces a Hamiltonian infinitesimal action compatible with this structure. We then construct explicit homotopy moment maps as \$L\_{\textbackslash}infty\$-morphisms from the Lie algebra \${\textbackslash}mathfrak\{g\}\$ into the Lie \$2\$-algebra of relative observables, extending the moment map formalism to the higher and relative geometric setting.},
	number = {{arXiv}:2509.08153},
	author = {Djounvouna, Dinamo},
	urldate = {2025-09-11},
	date = {2025-09-09},
	eprinttype = {arxiv},
	eprint = {2509.08153 [math]},
	keywords = {Mathematics - Symplectic Geometry},
	file = {PDF:/home/angel/Zotero/storage/NVJ7LDF5/Djounvouna - 2025 - Observables of Relative Structures and Lie 2-algebras associated with Quasi-Hamiltonian \$G\$-spaces.pdf:application/pdf},
}

